<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/moviesFragment2">

    <fragment
        android:id="@+id/moviesFragment2"
        android:name="com.rivas.diego.proyectorivas.ui.fragments.main.MoviesFragment"
        android:label="fragment_movies"
        tools:layout="@layout/fragment_movies" >
        <action
            android:id="@+id/action_moviesFragment2_to_profileSettingsFragment2"
            app:destination="@id/profileSettingsFragment2"
            app:enterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:exitAnim="@anim/car_ui_app_styled_view_exit_anim"
            app:popEnterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:popExitAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_moviesFragment2_to_searchFragment"
            app:destination="@id/searchFragment"
            app:enterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:exitAnim="@anim/car_ui_app_styled_view_exit_anim"
            app:popEnterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:popExitAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/profileSettingsFragment2"
        android:name="com.rivas.diego.proyectorivas.ui.fragments.login.ProfileSettingsFragment"
        android:label="fragment_profile_settings"
        tools:layout="@layout/fragment_profile_settings" >
        <action
            android:id="@+id/action_profileSettingsFragment2_to_moviesFragment2"
            app:destination="@id/moviesFragment2"
            app:enterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:exitAnim="@anim/car_ui_app_styled_view_exit_anim"
            app:popEnterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:popExitAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.rivas.diego.proyectorivas.ui.fragments.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_moviesFragment2"
            app:destination="@id/moviesFragment2"
            app:enterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:exitAnim="@anim/car_ui_app_styled_view_exit_anim"
            app:popEnterAnim="@anim/car_ui_app_styled_view_enter_anim"
            app:popExitAnim="@anim/nav_default_enter_anim" />
    </fragment>
</navigation>
